	}
	
	
	error.prec ~ dgamma(0.10,0.10)
	for (i in 1:p) {beta[i] ~ dnorm(0.0, 0.0001)}
	tausq <- 1/error.prec

	for (i in 1:NSITES) {muW[i] <- 0.0}
	W[1:NSITES] ~ spatial.exp(muW[], x[], y[], spat.prec, phi, 1) 	
	phi ~ dunif(0,10)	
	
	for (i in 1:NTIME) {muV[i] <- 0.0}
	V[1:NTIME] ~ spatial.exp(muV[],x.t[],y.t[],temporal.prec,phi.t,1)
	phi.t ~ dunif(0,10)

	spat.prec ~ dgamma(0.10, 0.10)
	sigmasq <- 1/spat.prec

	temporal.prec ~ dgamma(0.10,0.10)
	sigmasq.temp <- 1/temporal.prec
	
}


